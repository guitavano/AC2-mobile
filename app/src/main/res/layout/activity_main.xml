<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="156dp"
        android:layout_marginBottom="136dp"
        app:layout_constraintBottom_toTopOf="@+id/btnAbrirCadastroProfessor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/btnAbrirCadastroProfessor"
        android:layout_width="361dp"
        android:layout_height="51dp"
        android:layout_marginTop="80dp"
        android:layout_marginBottom="19dp"
        android:backgroundTint="#E87A72"
        android:text="Cadastrar Professor"
        app:layout_constraintBottom_toTopOf="@+id/btnPageAgenda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/btnPageAgenda"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="23dp"
        android:backgroundTint="#E87A72"
        android:text="Cadastrar Agenda"
        app:layout_constraintBottom_toTopOf="@+id/btnPageLista"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAbrirCadastroProfessor" />

    <Button
        android:id="@+id/btnPageLista"
        android:layout_width="360dp"
        android:layout_height="52dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#E87A72"
        android:text="Listar Agendas"
        app:layout_constraintBottom_toTopOf="@+id/btnImprimir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPageAgenda" />

    <ImageButton
        android:id="@+id/btnImprimir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#FF5722"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/stat_sys_download" />
</androidx.constraintlayout.widget.ConstraintLayout>